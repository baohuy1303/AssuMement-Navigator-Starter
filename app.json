{
  "expo": {
    "name": "AssuMement Navigator",
    "slug": "assumement-navigator",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.yourappname",
      "buildNumber": "1.0.0",
      "config": {
        "googleMapsApiKey":  "process.env.EXPO_PUBLIC_API_GOOGLE_MAPS"
        }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.assumementnavigator"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      
    ]
  }
}
